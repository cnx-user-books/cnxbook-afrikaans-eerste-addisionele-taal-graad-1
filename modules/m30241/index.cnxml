        <list id="id1167142839634" list-type="bulleted">
          <item>Luister na die begin van die woord.</item>
        </list>
        <figure id="id1167143352554">
          <media id="id1167143352554_media" alt="">
            <image mime-type="image/png" src="../../media/graphics12.png" id="id1167143352554__onlineimage" height="468" width="435"/>
          </media>
